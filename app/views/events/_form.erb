<%= form_for(@event) do |f| %>
  <% if @event.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@event.errors.count, "error") %> prohibited this event from being saved:</h2>

      <ul>
      <% @event.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
    <fieldset>
    <ol>
  <li>
    <%= f.label :name %>
    <%= f.text_field :name, :placeholder => "Title of the event" %>
  </li>
<li>
  <%= f.label :start_time %>
  <%= f.text_field :start_time, :placeholder => "start time (mm/dd/yy)" %>
</li>
      <li>
        <%= f.label :end_time %>
        <%= f.text_field :end_time, :placeholder => "end time (mm/dd/yy)" %>
      </li>
  <li>
    <%= f.label :description %>
    <%= f.text_area :description, :rows => 3 %>
  </li>
  <li>
    <%= f.label :event, "Upload event thumbnail(optional.)" %>
    <%= f.file_field :event %>
  <li>
   </ol>
  </fieldset>
  <div class="actions">
    <%= f.submit :class => "btn btn-primary" %>
  </div>
<% end %>

<script type="text/javascript">
    $(function() {
        $("#event_start_time").datepicker({
            dateFormat: "yy-mm-dd"
        });
    });
</script>
<script type="text/javascript">
    $(function() {
        $("#event_end_time").datepicker({
            dateFormat: "yy-mm-dd"
        });
    });
</script>