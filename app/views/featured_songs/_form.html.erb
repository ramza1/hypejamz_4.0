<%= form_for(@featured_song) do |f| %>
  <% if @featured_song.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@featured_song.errors.count, "error") %> prohibited this featured_song from being saved:</h2>

      <ul>
      <% @featured_song.errors.full_messages.each do |msg| %>
