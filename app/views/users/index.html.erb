<%= provide :title, t("user.members")%>
<%= provide(:right_nav) do %>
        <% if current_user %>
            <%= render "thumbnail_medium", :user => current_user %>
        <% else %>
            <%= render "not_member" %>
        <% end %>
    <hr>
    <%= render "right_nav" %>
<% end %>
<div class="page-header" style="margin-top: 0;">
  <h4>
        <i class="icon-user"></i> <%= t("user.members")%>
    <span class="right">
      <%= form_tag  users_search_box_path, :method => 'get', :class => "navbar-search"  do %>
      <%= text_field_tag :q, @q, :placeholder => "search for people", :class => "search-query span3" %>
  <% end %>
    </span>
    <div class="clear"></div>
  </h4>
</div>
<div id="songs">
      <%= render @users %>
</div>





